<!-- Section: Gallery -->
<section id="gallery" class="section section-gallery scrollspy">
  <div class="container">

    <h4 class="center">

    </h4>
<nav>
    <div class="nav-wrapper #ffeb3b yellow">
    <a href="/adopted" class="waves-effect waves-teal btn-flat">
      <span class="fa fa-th-list"></span> Adopted</a>

    <a href="/vaccinated" class="waves-effect waves-teal btn-flat">
      <span class="fa fa-th-list"></span> Vaccinated</a>  

    <a href="/gender/Male" class="waves-effect waves-teal btn-flat">
      <span class="fa fa-th-list"></span> Male</a>

    <a href="/gender/Female" class="waves-effect waves-teal btn-flat">
      <span class="fa fa-th-list"></span> Female</a>

    <a href="/all" class="waves-effect waves-teal btn-flat">
      <span class="fa fa-th-list"></span> Show all pets</a>
    </div>
  </nav>
   

    <div style="margin-bottom: 10px;"></div>

    <div class="row">
      {{#each dogs}}
      <div class="col s12 m4">
        <div class="card">
          <div class="card-image" style="display:flex; justify-content:center; ">
            <img src="{{photo}}" style="width:336.14px;height:224.09px; margin-top: 20px;">
            <span class="card-title"></span>
          </div>
          <div class="card-content">
            <p></p>
          </div>
          <div class="card-action" style="text-align:center;">
            
            <li href="#"> Name : {{dog_name}}</li>
            <li href="#"> Gender : {{gender}}</li>
            <li href="#"> Breed : {{Breed}}</li>
            <li href="#"> Vaccinated : <span style="text-transform: capitalize;">{{Vaccinated}}</span></li>
            <li href="#"> Size : {{size}}</li>
            <li href="#"> Gender: {{gender}}</li>
            <li href="#"> Adopted: {{Adapted}}</li>
            <a href="#" style="margin-right:0;">{{dog_name}}</a>
            
          </div>
        </div>
      </div>
      {{/each}}
    </div>

  </div>
</section>